{
	"name": "ts-function-inliner",
	"version": "0.2.0",
	"description": "TypeScript transformation that inlines calls to small functions. ⚡️",
	"repository": {
		"type": "git",
		"url": "https://github.com/JoshuaKGoldberg/ts-function-inliner"
	},
	"license": "MIT",
	"author": {
		"name": "JoshuaKGoldberg",
		"email": "npm@joshuakgoldberg.com"
	},
	"type": "module",
	"main": "./lib/index.js",
	"files": [
		"lib/",
		"package.json",
		"LICENSE.md",
		"README.md"
	],
	"scripts": {
		"build": "tsup",
		"format": "prettier .",
		"lint": "eslint . --max-warnings 0",
		"lint:knip": "knip",
		"lint:md": "markdownlint \"**/*.md\" \".github/**/*.md\" --rules sentences-per-line",
		"lint:packages": "pnpm dedupe --check",
		"lint:spelling": "cspell \"**\" \".github/**/*\"",
		"prepare": "husky",
		"should-semantic-release": "should-semantic-release --verbose",
		"test": "vitest",
		"tsc": "tsc"
	},
	"lint-staged": {
		"*": "prettier --ignore-unknown --write"
	},
	"dependencies": {
		"cached-factory": "^0.0.2"
	},
	"devDependencies": {
		"@eslint-community/eslint-plugin-eslint-comments": "^4.3.0",
		"@eslint/js": "^9.4.0",
		"@prettier/sync": "^0.5.0",
		"@release-it/conventional-changelog": "^8.0.1",
		"@types/eslint-plugin-markdown": "^2.0.2",
		"@vitest/coverage-v8": "^1.6.0",
		"console-fail-test": "^0.2.3",
		"cspell": "^8.8.3",
		"eslint": "^9.4.0",
		"eslint-plugin-jsdoc": "^48.2.7",
		"eslint-plugin-jsonc": "^2.16.0",
		"eslint-plugin-markdown": "^5.0.0",
		"eslint-plugin-n": "^17.7.0",
		"eslint-plugin-package-json": "^0.14.0",
		"eslint-plugin-perfectionist": "^2.10.0",
		"eslint-plugin-regexp": "^2.6.0",
		"eslint-plugin-vitest": "^0.5.4",
		"eslint-plugin-yml": "^1.14.0",
		"gulp": "^4.0.2",
		"gulp-typescript": "^6.0.0-alpha.1",
		"husky": "^9.0.11",
		"jsonc-eslint-parser": "^2.4.0",
		"knip": "^5.17.4",
		"lint-staged": "^15.2.5",
		"markdownlint": "^0.34.0",
		"markdownlint-cli": "^0.41.0",
		"prettier": "^3.3.0",
		"prettier-plugin-curly": "^0.2.1",
		"prettier-plugin-packagejson": "^2.5.0",
		"release-it": "^17.3.0",
		"sentences-per-line": "^0.2.1",
		"should-semantic-release": "^0.3.0",
		"tsup": "^8.1.0",
		"typescript": "^5.4.5",
		"typescript-eslint": "^7.11.0",
		"vitest": "^1.6.0"
	},
	"peerDependencies": {
		"typescript": "*"
	},
	"engines": {
		"node": ">=18"
	},
	"publishConfig": {
		"provenance": true
	}
}
